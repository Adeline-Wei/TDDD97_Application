CREATE TABLE IF NOT EXISTS Users (email TEXT PRIMARY KEY, password TEXT NOT NULL, firstname TEXT NOT NULL, familyname TEXT NOT NULL, gender TEXT NOT NULL, city TEXT NOT NULL, country TEXT NOT NULL);
CREATE TABLE IF NOT EXISTS Messages (mid INTEGER PRIMARY KEY AUTOINCREMENT, to_user TEXT NOT NULL, from_user TEXT NOT NULL, content TEXT NOT NULL);
CREATE TABLE IF NOT EXISTS Logins (lid INTEGER PRIMARY KEY AUTOINCREMENT, email TEXT, token TEXT NOT NULL);
CREATE TABLE IF NOT EXISTS Views (vid INTEGER PRIMARY KEY AUTOINCREMENT, email TEXT, times INTEGER NOT NULL);